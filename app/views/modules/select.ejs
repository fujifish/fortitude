<div class="form-group">
    <label><%- name %></label>
    <select aria-hidden="true" tabindex="-1" class="<%- name.split(' ')[0].toLowerCase() %> form-control select2 select2-hidden-accessible" style="width: 100%;">
        <% items.forEach(function(item) { %>
            <option><%- item %></option>
        <% }) %>
    </select>
</div>