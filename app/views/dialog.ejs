<div class="modal fade" id="<%= dialogId%>" tabIndex="-1" role="dialog">
  <div class="modal-dialog" role="document" style="<%=style%>">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title"><%= title %></h4>
        <%- header %>
      </div>
      <div class="modal-body">
        <%- body %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" id="<%= dialogId + '-cancel'%>">Cancel</button>
        <button type="button" class="btn btn-primary" id="<%= dialogId + '-ok'%>"><%= okLabel %></button>
      </div>
    </div>
  </div>
</div>
